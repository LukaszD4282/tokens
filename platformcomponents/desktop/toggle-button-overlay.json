{
  "toggleButtonOverlay": {
    "figma": "https://www.figma.com/file/NaNrfXjygZtRgMfHAFHjsp/Components---Windows?node-id=44859%3A103949&t=p8zl3QPbotGIIFT3-1",
    "comment": "Applied to circle and pill toggle buttons",
    "inactive": {
      "#normal": {
        "background": "@theme-button-inverted-normal",
        "border": "@theme-outline-button-normal",
        "text": "@theme-text-secondary-normal"
      },
      "#hovered": {
        "background": "@theme-button-inverted-hover",
        "border": "@theme-outline-button-normal",
        "text": "@theme-text-primary-normal"
      },
      "#pressed": {
        "background": "@theme-button-inverted-pressed",
        "border": "@theme-outline-button-normal",
        "text": "@theme-text-primary-normal"
      },
      "#disabled": {
        "background": "@theme-button-inverted-normal",
        "border": "@theme-outline-disabled-normal",
        "text": "@theme-text-primary-disabled"
      }
    },
    "active": {
      "#normal": {
        "background": "@theme-button-inverted-pressed",
        "border": "@theme-outline-button-normal",
        "text": "@theme-text-primary-normal"
      },
      "#hovered": {
        "background": "@theme-button-inverted-pressed",
        "border": "@theme-outline-button-normal",
        "text": "@theme-text-primary-normal"
      },
      "#pressed": {
        "background": "@theme-button-inverted-pressed",
        "border": "@theme-outline-button-normal",
        "text": "@theme-text-primary-normal"
      },
      "#disabled": {
        "background": "@theme-button-inverted-normal",
        "border": "@theme-outline-disabled-normal",
        "text": "@theme-text-primary-disabled"
      }
    }
  }
}
